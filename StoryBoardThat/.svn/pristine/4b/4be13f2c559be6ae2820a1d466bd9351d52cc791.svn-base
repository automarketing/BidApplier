body {
    font-family: "Open Sans", "Helvetica Neue", sans-serif; /*"Helvetica Neue", Helvetica, Arial, sans-serif;*/
    font-size: 15px;
    line-height: 1.5;
    font-weight: 400;
}

.logo {
    width: 220px;
    margin: 0 auto;
    display: block;
    text-align: center;
    background-color: white;
}

    .logo img {
        padding: 3px;
        max-width: 100%;
    }

ul#left-nav .logo-mob {
    padding: 2px;
}

header {
    /*height: 60px;
    line-height: 60px;*/
    
    padding: 0 10px;
    border-bottom: 1px solid #E5E7EA;
    margin-bottom: 5px;
    vertical-align: middle;
}

.btn-xlg,
.btn-group-xlg > .btn {
  padding: 12px 18px;
  font-size: 24px;
  line-height: 1.33;
  border-radius: 6px;
}

.centered-title
{
    text-align:center;
}

a
{
    color:#53acdf
}

.alert-info > a
{
    color:white;
    text-decoration:underline;
   
}
footer
{
    height:auto;
        background: #647782;
    color: #fff;
    padding: 35px 0;
    text-align: center;
    line-height:normal;
}

.top-callout-bar {
    
    text-align: center;
    
}
