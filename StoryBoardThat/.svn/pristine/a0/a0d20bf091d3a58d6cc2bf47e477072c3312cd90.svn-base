    min-height: 120px;
    padding: 8px;
    background: #e8f5ff;
    -webkit-border-radius: 10px;
    -moz-border-radius: 10px;
    border-radius: 10px;
    border: #243B53 solid 3px;
}

    .speech-bubble:after {
        content: '';
        position: absolute;
        border-style: solid;
        border-width: 5px 25px 5px 0;
        border-color: transparent #e8f5ff;
        display: block;
        width: 0;
        z-index: 1;
        left: -25px;
        top: 25%;
    }

    .speech-bubble:before {
        content: '';
        position: absolute;
        border-style: solid;
        border-width: 7px 27px 7px 0;
        border-color: transparent #243B53;
        display: block;
        width: 0;
        z-index: 0;
        left: -30px;
        top: 24%;
    }
/*#endregion*/

