///  <reference path="../../SvgManip.js" />
function AttributionArea(defaultFontSize)
{
    this.DefaultText = "";
    this.DefaultFontSize = defaultFontSize;
    this.DefaultFontColor = "#000000";
    this.DefaultFont = "Roboto";
    

    this.FillColor = new ColorableBox("none", "Textables", "Fill");

    this.Stroke = "none";
    this.StrokeWidth = 0;
    this.RoundedCornerRadius = 0;
    //this.InnerPadding = 3;
    this.TextPadding = 0;
    this.TextAlignment = "start";
    
    this.AllowEditting = false;
    this.ForceTextOverride = true;
    this.Rotation = 0;

    this.StoryboardPartType = StoryboardPartTypeEnum.StoryboardAttributionArea;

    
}